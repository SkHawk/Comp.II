#include "aula0301.h"

ull
CalcularTermoSerieFibonacci (unsigned short numero)
{
 
  if (numero <= 1)
    return numero;
  
  return (CalcularTermoSerieFibonacci(numero-1) + CalcularTermoSerieFibonacci(numero-2));
}

