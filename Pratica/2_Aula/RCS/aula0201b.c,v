#include "aula0201.h"

ull
CalcularMaximoDivisorComum (ull x, ull y)
{
  ull auxiliar;

  if ((x == 0) && (y == 0)) 
    return 0; 

  if (x == 0) 
    return y; 

  if (y == 0)
    return x;
  
  if (x%y == 0) 
    return y;
  do
  {
    x = x%y;
    auxiliar = x;
    x = y;
    y = auxiliar;
  } while (y != 0);

  return (x);
}

